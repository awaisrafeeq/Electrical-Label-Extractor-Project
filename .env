# API Keys - Copy this to .env and add your actual keys

# Choose one: OpenAI (GPT-4 Vision) or Anthropic (Claude Vision)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
# Vision AI Provider: "openai" or "anthropic"
VISION_PROVIDER=anthropic

# Model Selection
OPENAI_MODEL=gpt-4o
ANTHROPIC_MODEL=claude-3-haiku-20240307

# OCR Settings
OCR_LANGUAGE=en
OCR_USE_GPU=false

# Processing Settings
MAX_IMAGE_SIZE=2048
PDF_DPI=200

# Output Settings
OUTPUT_DIR=./output
TEMP_DIR=./temp
